{
  "name": "@querycap-ui/core.macro",
  "version": "0.4.4",
  "license": "MIT",
  "peerDependencies": {
    "@babel/runtime-corejs3": "*"
  },
  "dependencies": {
    "@babel/generator": "^7.16.0",
    "@babel/helper-module-imports": "^7.16.0",
    "@babel/types": "^7.16.0",
    "@emotion/serialize": "^1.0.2",
    "@querycap-ui/css-aliases": "workspace:^0.3.4",
    "babel-plugin-macros": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "../../node_modules/.bin/monobundle"
