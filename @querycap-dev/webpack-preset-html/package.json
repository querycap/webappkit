{
  "name": "@querycap-dev/webpack-preset-html",
  "version": "1.0.1",
  "license": "MIT",
  "peerDependencies": {
    "@babel/runtime-corejs3": "*",
    "@querycap-dev/devkit": "*",
    "webpack": "*"
  },
  "dependencies": {
    "@querycap/config": "workspace:*",
    "@types/workbox-webpack-plugin": "^5.1.8",
    "html-webpack-plugin": "^5.5.0",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-core": "^6.4.1",
    "workbox-navigation-preload": "^6.4.1",
    "workbox-precaching": "^6.4.1",
    "workbox-routing": "^6.4.1",
    "workbox-strategies": "^6.4.1",
    "workbox-webpack-plugin": "^6.4.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index-default.html",
    "service-worker-default.ts",
    "./dist/",
    "dist/"
  ],
  "scripts": {
    "prepublishOnly": "../../node_modules/.bin/monobundle"
  },
  "monobundle": {
    "sideDependencies": [
      "workbox-*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/querycap/webappkit.git"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./index.ts": "./index.ts"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs"
}
