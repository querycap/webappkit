{
  "name": "@querycap-ui/core",
  "version": "0.11.8",
  "license": "MIT",
  "peerDependencies": {
    "@babel/runtime-corejs3": "*",
    "lodash": "4.x",
    "react": "17.x"
  },
  "dependencies": {
    "@emotion/react": "^11.6.0",
    "@querycap-ui/core.macro": "workspace:^0.4.5",
    "@querycap-ui/css-aliases": "workspace:^0.3.4",
    "@use-gesture/react": "^10.1.6",
    "polished": "^4.1.3",
    "react-spring": "^9.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "monobundle": {
    "env": "browser",
    "sideDependencies": [
      "@querycap-ui/core.macro"
    ]
  },
  "scripts": {
    "prepublishOnly": "../../node_modules/.bin/monobundle"
  },
  "files": [
    "jsx-runtime/",
    "macro/",
    "./dist/",
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/querycap/webappkit.git"
  },
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./jsx-runtime": {
      "require": "./jsx-runtime/index.js",
      "import": "./jsx-runtime/index.mjs"
    },
    "./macro": {
      "require": "./macro/index.js",
      "import": "./macro/index.js"
    },
    "./index.ts": "./index.ts"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs"
}
